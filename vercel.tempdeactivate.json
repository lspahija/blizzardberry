{
  "crons": [
    {
      "path": "/api/credits/release-expired-holds",
      "schedule": "*/5 * * * *",
      "headers": {
        "Authorization": "Bearer ${CRON_SECRET}"
      }
    },
    {
      "path": "/api/credits/expire",
      "schedule": "0 0 * * *",
      "headers": {
        "Authorization": "Bearer ${CRON_SECRET}"
      }
    },
    {
      "path": "/api/events/process-events",
      "schedule": "*/1 * * * *",
      "headers": {
        "Authorization": "Bearer ${CRON_SECRET}"
      }
    },
    {
      "path": "/api/events/retry-events",
      "schedule": "0 * * * * *",
      "headers": {
        "Authorization": "Bearer ${CRON_SECRET}"
      }
    }
  ]
}